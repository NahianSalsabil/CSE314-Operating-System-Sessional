+void
+SwapOut(void){
+  struct proc *curproc = myproc();
+  int ind;
+  if(algoflag == 1)   // FIFO
+    ind = 0;
+  else if(algoflag ==  2){     // AGING
+    ind = FindMinimumCount();
+    cprintf("min count index: %d\n", ind);
+  }
+  
+  uint addr = curproc->phy_page_list[ind].virtual_address;
+
+  if(algoflag == 1){
+    for(int i = 0; i < MAX_PSYC_PAGES-1; i++){
+      curproc->phy_page_list[i].index = curproc->phy_page_list[i+1].index;
+      curproc->phy_page_list[i].virtual_address = curproc->phy_page_list[i+1].virtual_address;
+      curproc->phy_page_list[i].used = curproc->phy_page_list[i+1].used;
+    }
+  }
+  else if(algoflag == 2){
+    curproc->phy_page_list[ind].used = 0;
+  }
+  
+  curproc->phymempagecount--;
+
+  if(curproc->swapfilepagecount == 15){
+    panic("MAX TOTAL PAGES reached\n");
+  }
+  curproc->swap_file_page_list[curproc->swapfilepagecount].index = curproc->swapfilepagecount;
+  curproc->swap_file_page_list[curproc->swapfilepagecount].virtual_address = addr;
+
+  pte_t *pte;
+  pte = walkpgdir(curproc->pgdir, (char*)addr, 0);
+  char* va= (char*)P2V(PTE_ADDR(*pte));
+
+  if(writeToSwapFile(curproc, va, curproc->swapfilepagecount*PGSIZE, PGSIZE) != -1){
+    curproc->swapfilepagecount++;
+
+    *pte |= PTE_PG;
+    *pte &= ~PTE_P;
+    kfree(va);
+  }
+  cprintf("Swapped Out\n");
+  lcr3(V2P(curproc->pgdir));
+}
+
+void
+SwapIn(uint addr){
+  struct proc *curproc = myproc();
+  uint naddr = PGROUNDDOWN(addr);
+  pte_t* pte;
+  
+  char* mem;
+  mem = kalloc();
+  memset(mem, 0, PGSIZE);
+
+  if(mappages(curproc->pgdir, (char*)naddr, PGSIZE, V2P(mem), PTE_W|PTE_U) < 0){
+    cprintf("not mapped\n");
+    return;
+  }
+
+  pte = walkpgdir(curproc->pgdir, (char*)naddr, 0);
+  *pte &= ~PTE_PG;
+
+  lcr3(V2P(curproc->pgdir));
+
+  char* va = (char*)P2V(PTE_ADDR(*pte));
+
+  int temp_i = -1;
+
+  for(int i = 0; i < MAX_TOTAL_PAGES-MAX_PSYC_PAGES; i++){
+    if(naddr == curproc->swap_file_page_list[i].virtual_address){
+      if(readFromSwapFile(curproc, va, curproc->swap_file_page_list[i].index*PGSIZE, PGSIZE) != -1){
+        cprintf("Swapped In\n");
+
+        if(algoflag == 1){
+          curproc->phy_page_list[MAX_PSYC_PAGES-1].virtual_address = naddr;
+        }
+
+        else if(algoflag == 2){
+          for(int j = 0; j < MAX_PSYC_PAGES; j++){
+            if(curproc->phy_page_list[i].used == 0){
+              curproc->phy_page_list[i].virtual_address = naddr;
+              curproc->phy_page_list[i].used = 1;
+              curproc->phy_page_list[i].counter = 0;
+              break;
+            }
+          }
+        }
+        temp_i = i;
+      }
+      break;
+    }
+  }
+  for(int i = temp_i; i < MAX_TOTAL_PAGES-MAX_PSYC_PAGES-1; i++){
+    curproc->swap_file_page_list[i].index = curproc->swap_file_page_list[i+1].index;
+    curproc->swap_file_page_list[i].virtual_address = curproc->swap_file_page_list[i+1].virtual_address;
+  }
+
+  curproc->phymempagecount++;
+  curproc->swapfilepagecount--;
+}
+
+void
+FIFO(uint addr){
+  struct proc *curproc = myproc();
+  if(curproc->phymempagecount == 15){
+    SwapOut();
+  }
+  SwapIn(addr);
+}
+
+void
+AGING(uint addr){
+  struct proc *curproc = myproc();
+  if(curproc->phymempagecount == 15){
+    SwapOut();
+  }
+  SwapIn(addr);
+}
